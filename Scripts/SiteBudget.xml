							<SharePoint:FormField runat="server" id="ff7{$Pos}" ControlMode="Display" FieldName="Year" __designer:bind="{ddwrt:DataBind('u',concat('ff7',$Pos),'Value','ValueChanged','ID',ddwrt:EscapeDelims(string(@ID)),'@Year')}"/>
