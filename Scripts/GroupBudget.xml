								<nobr>Name</nobr>
							</H3>
						</td>
						<td width="400px" valign="top" class="ms-formbody">
							<SharePoint:FormField runat="server" id="ff10{$Pos}" ControlMode="Display" FieldName="FileLeafRef" __designer:bind="{ddwrt:DataBind('u',concat('ff10',$Pos),'Value','ValueChanged','ID',ddwrt:EscapeDelims(string(@ID)),'@FileLeafRef')}"/>
							<SharePoint:FieldDescription runat="server" id="ff10description{$Pos}" FieldName="FileLeafRef" ControlMode="Display"/>
						</td>
					   </tr>
					   <tr>
						<td width="190px" valign="top" class="ms-formlabel">
							<H3 class="ms-standardheader">
								<nobr>Year</nobr>
							</H3>
						</td>
						<td width="400px" valign="top" class="ms-formbody">
							<SharePoint:FormField runat="server" id="ff3{$Pos}" ControlMode="Display" FieldName="Year0" __designer:bind="{ddwrt:DataBind('u',concat('ff3',$Pos),'Value','ValueChanged','ID',ddwrt:EscapeDelims(string(@ID)),'@Year0')}"/>
							<SharePoint:FieldDescription runat="server" id="ff3description{$Pos}" FieldName="Year0" ControlMode="Display"/>
						</td>
					   </tr>
					   <tr>
						<td width="190px" valign="top" class="ms-formlabel">
							<H3 class="ms-standardheader">
								<nobr>Description</nobr>
							</H3>
						</td>
						<td width="400px" valign="top" class="ms-formbody">
							<SharePoint:FormField runat="server" id="ff1{$Pos}" ControlMode="Display" FieldName="DocumentSetDescription" __designer:bind="{ddwrt:DataBind('u',concat('ff1',$Pos),'Value','ValueChanged','ID',ddwrt:EscapeDelims(string(@ID)),'@DocumentSetDescription')}"/>
							<SharePoint:FieldDescription runat="server" id="ff1description{$Pos}" FieldName="DocumentSetDescription" ControlMode="Display"/>
						</td>
					   </tr>
					   <tr>
						<td width="190px" valign="top" class="ms-formlabel">
							<H3 class="ms-standardheader">
								<nobr>Group Doc Status</nobr>
							</H3>
						</td>
						<td width="400px" valign="top" class="ms-formbody">
							<SharePoint:FormField runat="server" id="ff12{$Pos}" ControlMode="Display" FieldName="Group_x0020_Doc_x0020_Status0" __designer:bind="{ddwrt:DataBind('u',concat('ff12',$Pos),'Value','ValueChanged','ID',ddwrt:EscapeDelims(string(@ID)),'@Group_x0020_Doc_x0020_Status0')}"/>
							<SharePoint:FieldDescription runat="server" id="ff12description{$Pos}" FieldName="Group_x0020_Doc_x0020_Status0" ControlMode="Display"/>
						</td>
					   </tr>
					   <tr>
						<td width="190px" valign="top" class="ms-formlabel">
							<H3 class="ms-standardheader">
								<nobr>Group Budget Reviewer's Approval</nobr>
							</H3>
						</td>
						<td width="400px" valign="top" class="ms-formbody">
						     <SharePoint:FormField runat="server" id="ff11{$Pos}" ControlMode="Edit" FieldName="GBR_ApprovalStatus0" __designer:bind="{ddwrt:DataBind('u',concat('ff11',$Pos),'Value','ValueChanged','ID',ddwrt:EscapeDelims(string(@ID)),'@GBR_ApprovalStatus0')}"/>
								<SharePoint:FieldDescription runat="server" id="ff11description{$Pos}" FieldName="GBR_ApprovalStatus0" ControlMode="Edit"/>							
						</td>
					   </tr>
					   <tr>
						<td width="190px" valign="top" class="ms-formlabel">
							<H3 class="ms-standardheader">
								<nobr>Comment</nobr>
							</H3>
						</td>
						        <td width="400px" valign="top" class="ms-formbody">
									<SharePoint:FormField runat="server" id="ff2{$Pos}" ControlMode="Edit" FieldName="Comment1" __designer:bind="{ddwrt:DataBind('u',concat('ff2',$Pos),'Value','ValueChanged','ID',ddwrt:EscapeDelims(string(@ID)),'@Comment1')}"/>
									<SharePoint:FieldDescription runat="server" id="ff2description{$Pos}" FieldName="Comment1" ControlMode="Edit"/>
								</td>
					   </tr>
					   <tr id="idAttachmentsRow">
						<td nowrap="true" valign="top" class="ms-formlabel" width="20%">
							<SharePoint:FieldLabel ControlMode="Edit" FieldName="Attachments" runat="server"/>
						</td>
						<td valign="top" class="ms-formbody" width="80%">
							<SharePoint:FormField runat="server" id="AttachmentsField" ControlMode="Edit" FieldName="Attachments" __designer:bind="{ddwrt:DataBind('u','AttachmentsField','Value','ValueChanged','ID',ddwrt:EscapeDelims(string(@ID)),'@Attachments')}"/>
							<script>
          						var elm = document.getElementById(&quot;idAttachmentsTable&quot;);
						          if (elm == null || elm.rows.length == 0)
						          document.getElementById(&quot;idAttachmentsRow&quot;).style.display=&apos;none&apos;;
						        </script>
						</td>
					    </tr>
							<xsl:if test="$dvt_1_automode = '1'" ddwrt:cf_ignore="1">
						<tr>
							<td colspan="99" class="ms-vb">
								<span ddwrt:amkeyfield="ID" ddwrt:amkeyvalue="ddwrt:EscapeDelims(string(@ID))" ddwrt:ammode="view"></span>
							</td>
						</tr>
					  </xsl:if>
				   </xsl:when>
				   		<xsl:otherwise>
				   		<tr>
						<td width="190px" valign="top" class="ms-formlabel">
							<H3 class="ms-standardheader">
								<nobr>Name</nobr>
							</H3>
						</td>
						<td width="400px" valign="top" class="ms-formbody">
							<SharePoint:FormField runat="server" id="ff20{$Pos}" ControlMode="Display" FieldName="FileLeafRef" __designer:bind="{ddwrt:DataBind('u',concat('ff20',$Pos),'Value','ValueChanged','ID',ddwrt:EscapeDelims(string(@ID)),'@FileLeafRef')}"/>
							<SharePoint:FieldDescription runat="server" id="ff20description{$Pos}" FieldName="FileLeafRef" ControlMode="Display"/>
						</td>
					</tr>
					<tr>
						<td width="190px" valign="top" class="ms-formlabel">
							<H3 class="ms-standardheader">
								<nobr>Year</nobr>
							</H3>
						</td>
						<td width="400px" valign="top" class="ms-formbody">
							<SharePoint:FormField runat="server" id="ff21{$Pos}" ControlMode="display" FieldName="Year0" __designer:bind="{ddwrt:DataBind('u',concat('ff21',$Pos),'Value','ValueChanged','ID',ddwrt:EscapeDelims(string(@ID)),'@Year0')}"/>
							<SharePoint:FieldDescription runat="server" id="ff21description{$Pos}" FieldName="Year0" ControlMode="Display"/>
						</td>
					</tr>
					<tr>
						<td width="190px" valign="top" class="ms-formlabel">
							<H3 class="ms-standardheader">
								<nobr>Description</nobr>
							</H3>
						</td>
						<td width="400px" valign="top" class="ms-formbody">
							<SharePoint:FormField runat="server" id="ff22{$Pos}" ControlMode="Display" FieldName="DocumentSetDescription" __designer:bind="{ddwrt:DataBind('u',concat('ff22',$Pos),'Value','ValueChanged','ID',ddwrt:EscapeDelims(string(@ID)),'@DocumentSetDescription')}"/>
							<SharePoint:FieldDescription runat="server" id="ff22description{$Pos}" FieldName="DocumentSetDescription" ControlMode="Display"/>
						</td>
					</tr>
					<tr>
						<td width="190px" valign="top" class="ms-formlabel">
							<H3 class="ms-standardheader">
								<nobr>Group Doc Status</nobr>
							</H3>
						</td>
						<td width="400px" valign="top" class="ms-formbody">
							<SharePoint:FormField runat="server" id="ff23{$Pos}" ControlMode="Display" FieldName="Group_x0020_Doc_x0020_Status0" __designer:bind="{ddwrt:DataBind('u',concat('ff23',$Pos),'Value','ValueChanged','ID',ddwrt:EscapeDelims(string(@ID)),'@Group_x0020_Doc_x0020_Status0')}"/>
							<SharePoint:FieldDescription runat="server" id="ff23description{$Pos}" FieldName="Group_x0020_Doc_x0020_Status0" ControlMode="Display"/>
						</td>
					</tr>
					<tr>
						<td width="190px" valign="top" class="ms-formlabel">
							<H3 class="ms-standardheader">
								<nobr>Group Budget Reviewer's Approval</nobr>
							</H3>
						</td>
						<td width="400px" valign="top" class="ms-formbody">
								<SharePoint:FormField runat="server" id="ff24{$Pos}" ControlMode="Display" FieldName="GBR_ApprovalStatus0" __designer:bind="{ddwrt:DataBind('u',concat('ff24',$Pos),'Value','ValueChanged','ID',ddwrt:EscapeDelims(string(@ID)),'@GBR_ApprovalStatus0')}"/>
								<SharePoint:FieldDescription runat="server" id="ff24description{$Pos}" FieldName="GBR_ApprovalStatus0" ControlMode="Display"/>							
						</td>
					</tr>
					<tr>
						<td width="190px" valign="top" class="ms-formlabel">
							<H3 class="ms-standardheader">
								<nobr>Comment</nobr>
							</H3>
						</td>		 	
						<td width="400px" valign="top" class="ms-formbody">
							<xsl:value-of select="@Comment1"/>
						</td>
					</tr>
					<tr id="idAttachmentsRow">
						<td nowrap="true" valign="top" class="ms-formlabel" width="20%">
							<SharePoint:FieldLabel ControlMode="Display" FieldName="Attachments" runat="server"/>
						</td>
						<td valign="top" class="ms-formbody" width="80%">
							<SharePoint:FormField runat="server" id="AttachmentsField" ControlMode="Display" FieldName="Attachments" __designer:bind="{ddwrt:DataBind('u','AttachmentsField','Value','ValueChanged','ID',ddwrt:EscapeDelims(string(@ID)),'@Attachments')}"/>
							<script>
         						 var elm = document.getElementById(&quot;idAttachmentsTable&quot;);
          					     if (elm == null || elm.rows.length == 0)
         					     document.getElementById(&quot;idAttachmentsRow&quot;).style.display=&apos;none&apos;;
        					</script>
						</td>
					</tr>
							<xsl:if test="$dvt_1_automode = '1'" ddwrt:cf_ignore="1">
						<tr>
							<td colspan="99" class="ms-vb">
								<span ddwrt:amkeyfield="ID" ddwrt:amkeyvalue="ddwrt:EscapeDelims(string(@ID))" ddwrt:ammode="view"></span>
							</td>
						</tr>
					</xsl:if>
				   </xsl:otherwise>
			       	</xsl:choose>
				</table>
			</td>
		</tr>
	</xsl:template>
</xsl:stylesheet>	</Xsl>

 

